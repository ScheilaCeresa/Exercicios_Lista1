algoritmo "exer.13"
// Disciplina   : [Linguagem e Lógica de Programação] 
// Professor   : Antonio Carlos Nicolodi 
var
   m1, m2, m3, r12, r13, r23, ee, g: real
inicio
      escreva ("Informe as tres massas: ")
      leia (m1,m2,m3)
      escreva ("Informe as tres distancias: ")
      leia (r12, r13, r23)
      
      g <- 6.67*10^(-11)
      
      ee <- g*(((m1*m2)/r12)+((m1*m3)/r13)+((m2*m3)/r23))
      
      escreva ("Valor de coesão: ", ee)
      
fimalgoritmo