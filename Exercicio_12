algoritmo "exer.12"
var
   a, b, c, d, ee, f, x, y: real
inicio
      escreva("Informe seis valores numericos respectivamente: ")
      leia (a, b, c, d, ee, f)
      
      x <- (((c*ee)-(b*f)) / ((a*ee)-(b*d)))
      y <- (((a*f)-(c*d)) /  ((a*ee)-(b*d)))
      
      c <- (a*x) + (b*y)
      f <- (d*x) + (ee*y)
      
      escreval("Valor de X: ",x, " Valor de Y: ", y)
      escreva ("Prova real de C: ", c, " E do F: ", f)
fimalgoritmo